{
  "name": "only-books",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build --turbopack",
    "start": "next start",
    "check:lint": "biome lint --write ./",
    "check:format": "biome format --write ./",
    "check:imports": "biome check --formatter-enabled=false --linter-enabled=false --write ./",
    "check:ts": "tsc --noEmit",
    "check:all": "pnpm check:imports && pnpm check:format && pnpm check:lint && pnpm check:ts",
    "prepare": "husky install",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "next": "15.5.3",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-icons": "^5.5.0"
  },
  "devDependencies": {
    "@biomejs/biome": "2.2.4",
    "@storybook/react": "^9.1.5",
    "@storybook/nextjs": "^9.1.5",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "husky": "^8.0.0",
    "storybook": "^9.1.5",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
